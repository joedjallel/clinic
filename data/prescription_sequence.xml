<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Prescriptions -->
        <record id="seq_clinic_prescription" model="ir.sequence">
            <field name="name">Prescription Sequence</field>
            <field name="code">clinic.prescription</field>
            <field name="prefix">PRESC/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>